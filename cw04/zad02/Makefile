CC     := gcc
CFLAGS := -g2 -Wall

.PHONY: all clean

all: SA_NODEFER SA_RESETHAND SA_SIGINFO

SA_NODEFER: SA_NODEFER.c
	$(CC) $(CFLAGS) SA_NODEFER.c -o SA_NODEFER

SA_RESETHAND: SA_RESETHAND.c
	$(CC) $(CFLAGS) SA_RESETHAND.c -o SA_RESETHAND

SA_SIGINFO: SA_SIGINFO.c
	$(CC) $(CFLAGS) SA_SIGINFO.c -o SA_SIGINFO

clean:
	$(RM) SA_NODEFER SA_RESETHAND SA_SIGINFO
